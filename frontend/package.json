{
  "name": "react-quickstart-template",
  "version": "1.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "format-check": "prettier '{*,{src}/**/*}.{ts,tsx,js,jsx,html,json,css,yml}' --check",
    "lint-check": "tslint --project tsconfig.json --config tslint.json '{src}/**/*.{ts,tsx}'",
    "check": "npm run format-check && npm run type-check && npm run lint-check",
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --mode=development"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^14.14.6",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-define": "^2.0.0",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^5.2.1",
    "html-webpack-plugin": "4.5.0",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "ts-lint": "^4.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^5.0.0",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1"
  }
}
